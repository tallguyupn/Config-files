general.import = [

"~.config/alacritty/theme.toml"
]

[window]
decorations = "None"
opacity = 0.7
blur = false

[window.padding]
x = 10
y = 10

[font]
size = 11.0

[font.normal]
 family = "CaskaydiaCove Nerd Font Mono"
 style = "Regular"

[cursor.style]
shape = "Underline"
blinking = "Always"

[selection]
save_to_clipboard = true

[terminal.shell]
program = "/usr/bin/zsh"

